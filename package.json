{
  "name": "@dialstack/sdk",
  "version": "0.2.2-alpha.13",
  "description": "Official JavaScript SDK for DialStack - Business Voice for Vertical SaaS",
  "keywords": [
    "dialstack",
    "voice",
    "telephony",
    "sip",
    "webrtc",
    "embedded-components",
    "react"
  ],
  "license": "MIT",
  "type": "module",
  "author": "DialStack",
  "homepage": "https://github.com/dialstack/dialstack-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dialstack/dialstack-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/dialstack/dialstack-sdk/issues"
  },
  "main": "dist/sdk.cjs",
  "module": "dist/sdk.mjs",
  "unpkg": "dist/sdk.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sdk.mjs",
      "require": "./dist/sdk.cjs",
      "types": "./dist/index.d.ts"
    },
    "./pure": {
      "import": "./dist/pure.mjs",
      "require": "./dist/pure.cjs",
      "types": "./dist/pure.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run clean && npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.9.0",
    "ts-jest": "^29.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.29"
  }
}
